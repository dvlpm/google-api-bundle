{
    "name": "dvlpm/google-api-bundle",
    "description": "Google API Client Symfony Bundle",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Richard Gerdes",
            "email": "rich@richgerdes.com"
        }
    ],
    "autoload": {
        "psr-4": { "DoubleStarSystems\\Bundle\\GoogleApiBundle\\": "src" }
    },
    "require": {
        "google/apiclient": "^2.0",
        "symfony/framework-bundle": "~4.0||~5.0||~6.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15",
        "phpcompatibility/php-compatibility": "^8.1",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
